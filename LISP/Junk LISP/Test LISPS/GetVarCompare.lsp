;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;Saves length of variables in lstvar.
(setq lstlngth (length lstvar))

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;Lists the return values of all system variables.

(setq lstret
	(mapcar '(lambda (x) 
		(getvar x)
		)
	lstvar
	)
)


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;Groups the system variable (lstvar) with the return value (lstret).

(setq counter 0)
(repeat lstlngth

	(setq lstpair (cons (nth counter lstvar) (nth counter lstret)))
	(setq lstgrp (cons lstpair lstgrp))

(setq counter (1+ counter))
)

